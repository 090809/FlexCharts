<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:Material.Controls.Popups">
  <ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="Material.Drawables.xaml"/>
		<ResourceDictionary Source="Material.Shadows.xaml"/>
	</ResourceDictionary.MergedDictionaries>
  <Style TargetType="popups:FavoritesPopup">
		<Setter Property="Effect" Value=""/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="popups:FavoritesPopup">
          <Border 
						Background="{TemplateBinding Background}"
						Effect="{TemplateBinding Effect}">
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
                <RowDefinition/>
                <RowDefinition Height="30"/>
              </Grid.RowDefinitions>
							
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>